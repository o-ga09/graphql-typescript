{
  "name": "graphql-typescript",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "transpile": "tsc",
    "start": "npm run transpile && node dist/app.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'tsx' src/app.ts",
    "generate:server": "npx graphql-codegen",
    "lint": "eslint . -c eslint.config.js --report-unused-disable-directives --max-warnings 0",
    "format": "npx prettier . --check",
    "format:fix": "npx prettier --write .",
    "eslint:fix": "eslint . --fix",
    "test": "vitest",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "^4.10.4",
    "@graphql-tools/graphql-file-loader": "^8.0.1",
    "@graphql-tools/load": "^8.0.2",
    "@graphql-tools/schema": "^10.0.4",
    "apollo-server-express": "^3.13.0",
    "bcrypt": "^5.1.1",
    "graphql": "^16.8.2",
    "jsonwebtoken": "^9.0.2",
    "ulid": "^2.3.0",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/introspection": "4.0.3",
    "@graphql-codegen/typescript": "4.0.9",
    "@graphql-codegen/typescript-document-nodes": "4.0.9",
    "@graphql-codegen/typescript-resolvers": "4.2.1",
    "@types/node": "^20.12.0",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.9.1",
    "globals": "^15.9.0",
    "mysql2": "^3.11.3",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "testcontainers": "^10.11.0",
    "tsx": "^4.17.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.2.0",
    "vite-plugin-env-compatible": "^2.0.1",
    "vitest": "^2.0.5"
  }
}
